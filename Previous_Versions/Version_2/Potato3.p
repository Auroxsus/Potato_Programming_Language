|| Arithmetic operators ||
|[ -------------------- ]|
   FRY " 1 = ",1,PEEL.
   FRY "+2 = ",+2,PEEL.
   FRY "-3 = ",-3,PEEL.
   FRY "ABS 2 = ",ABS 2,PEEL.
   FRY "ABS(-3) = ",ABS(-3),PEEL.
   FRY "4+5 = ",(4+5),PEEL.
   FRY "5-4 = ",5-4,PEEL.
   FRY "6*7 = ",6*7,PEEL.
   FRY "7/4 = ",7/4,PEEL.
   FRY "7%4 = ",7%4,PEEL.
   FRY "2^3 = ",2^3,PEEL.
   FRY "2^(3**2) = ",2^(3**2),PEEL.
