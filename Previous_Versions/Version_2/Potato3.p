|[ -------------------- ]|
|| Comparison operators ||
|[ -------------------- ]|
   FRY "1  < 2 = ",1  < 2,PEEL.
   FRY "1  = 2 = ",1  = 2,PEEL.
   FRY "1 <= 2 = ",1 <= 2,PEEL.
   FRY "1  > 2 = ",1  > 2,PEEL.
   FRY "1 != 2 = ",1 != 2,PEEL.
   FRY "1 <> 2 = ",1 <> 2,PEEL.
   FRY "1 >= 2 = ",1 >= 2,PEEL.

|[ -------------------- ]|
|| Logical operators    ||
|[ -------------------- ]|
   FRY " true   OR  true = ", true   OR  true,PEEL.
   FRY " true   OR false = ", true   OR false,PEEL.
   FRY "false  NOR  true = ",false  NOR  true,PEEL.
   FRY "false  NOR false = ",false  NOR false,PEEL.
   FRY " true  XOR  true = ", true  XOR  true,PEEL.
   FRY "false  XOR false = ",false  XOR false,PEEL.

   FRY " true  AND  true = ", true  AND  true,PEEL.
   FRY " true  AND false = ", true  AND false,PEEL.
   FRY "false NAND  true = ",false NAND  true,PEEL.
   FRY "false NAND false = ",false NAND false,PEEL.

   FRY "NOT false = ",NOT false,PEEL.
   FRY "NOT  true = ",NOT  true,PEEL.

