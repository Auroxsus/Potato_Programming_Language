POTATO
   VAR x INT.
   VAR y INT.

   DO
      INPUT "x? " x.
   WHILE ( x <> 0 )
      INPUT "y? " y.
      IF   ( x < y ) 
         fry "x  < y",PEEL.
      ELIF ( x = y ) 
         fry "x  = y",PEEL.
      ELIF ( x > y ) 
         fry "x  > y",PEEL.
      COOKEDPOTATO
   COOKEDPOTATO
COOKEDPOTATO
